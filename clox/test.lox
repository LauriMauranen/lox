fun kello() {
	var aika = 0;
	fun lisaaAika() {
		aika = aika + 1;
		print aika
	}
	fun vahennaAika() {
		aika = aika - 1;
		print aika;
	}
	fun tee(a) {
		if (a) {
			lisaaAika();
		} else {
			vahennaAika();
		}
	}
	return tee
}

var tee = kello();
tee(true); // 1
tee(false); // 0

